<div class="page_content">
	<div class="container">
		<div class="row row-lg-eq-height">

			<!-- Main Content -->

			<div class="col-lg-9">
				<div class="main_content">

					<!-- Category -->

					<div class="category">
						<div class="section_panel d-flex flex-row align-items-center justify-content-start">
							<div class="section_title">Search</div>
						</div>
						<div class="section_content">
							<div class="grid clearfix">
								{{#if this.emptyPost}}
								<div class="side_post">
									<a href="/">
										<div
											class="d-flex flex-row align-items-xl-center align-items-start justify-content-start">
											<div class="side_post_title">Non Post</div>
										</div>
									</a>
								</div>
								{{else}}
								<div class="owl-item">
									{{#each this.listPost}}
									<!-- Small Card With Image -->
										{{#if IsPremium}}
											<div class="card card_default card_small_with_image grid-item" style="background-color: rgb(231, 231, 146);">
												<img class="card-img-top" src="{{Avatar}}" alt="">
												<div class="card-body">
													<div class="card-title card-title-small"><a href="/detail/premium/{{Url}}"><span style="color: red;">{{Title}}</span>  &nbsp;<i class="fas fa-crown"></i></a></div>
													<p class="card-text"></p>{{Content_Summary}}</p>
													<small class="post_meta"><a href="#">{{Nickname}}</a><span>{{DatetimePost}}</span></small>
													<li class="post_tag" style="background-color: rgb(173, 57, 57);"><a style="color:seashell" href="/category/{{CatURL}}">{{Name}}</a></li><br>
													{{#load_list_tags ../listPostTags Id}}
														<li class="post_tag" style="background-color: rgb(161, 179, 61);"><a href="/tag/{{TagName}}">{{Name}}</a></li>
													{{/load_list_tags}}
												</div>
											</div>
										{{else}}
											<div class="card card_default card_small_with_image grid-item">
												<img class="card-img-top" src="{{Avatar}}" alt="">
												<div class="card-body">
													<div class="card-title card-title-small"><a href="/detail/{{Url}}">{{Title}}</a></div>
													<p class="card-text"></p>{{Content_Summary}}</p>
													<small class="post_meta"><a href="#">{{Nickname}}</a><span>{{DatetimePost}}</span></small>
													<li class="post_tag" style="background-color: rgb(173, 57, 57);"><a style="color:seashell" href="/category/{{CatURL}}">{{Name}}</a></li><br>
													{{#load_list_tags ../listPostTags Id}}
														<li class="post_tag" style="background-color: rgb(161, 179, 61);"><a href="/tag/{{TagName}}">{{Name}}</a></li>
													{{/load_list_tags}}
												</div>
											</div>
										{{/if }}
									{{/each}}
								</div>
								{{/if}}
							</div>


							<div class="row justify-content-center">
								<nav aria-label="...">
									<ul class="pagination">
										<li class="page-item {{#unless can_go_prev}}disabled{{/unless}}">
											<a class="page-link"
												{{#if SearchNotEmpty}}href="/search?page=1&Search={{Search}}"
												{{else}}href="/search?page=1"
												{{/if}} aria-label="Previous">
												<span aria-hidden="true">FIRST</span>
											</a>
										</li>
										<li class="page-item {{#unless can_go_prev}}disabled{{/unless}}">
											<a class="page-link"
												{{#if SearchNotEmpty}}href="/search?page={{prev_value}}&Search={{Search}}"
												{{else}}href="/search?page={{prev_value}}"
												{{/if}} aria-label="Previous">
												<span aria-hidden="true">&laquo;</span>
											</a>
										</li>
										{{#each this.page_items}}
										{{#if isActive}}
										<li class="page-item active" aria-current="page">
											<span class="page-link">
												{{value}}
												<span class="sr-only">(current)</span>
											</span>
										</li>
										{{else}}
										<li class="page-item {{#if isDisabled}}disabled{{/if}}">
											<a class="page-link"
												{{#if ../SearchNotEmpty}}href="/search?page={{value}}&Search={{../Search}}"
												{{else}}href="/search?page={{value}}"
												{{/if}}>{{value}}</a>
										</li>
										{{/if}}
										{{/each}}
										<li class="page-item {{#unless can_go_next}}disabled{{/unless}}">
											<a class="page-link"
												{{#if SearchNotEmpty}}href="/search?page={{next_value}}&Search={{Search}}"
												{{else}}href="/search?page={{next_value}}"
												{{/if}} aria-label="Next">
												<span aria-hidden="true">&raquo;</span>
											</a>
										</li>
										<li class="page-item {{#unless can_go_next}}disabled{{/unless}}">
											<a class="page-link"
												{{#if SearchNotEmpty}}href="/search?page={{last}}&Search={{Search}}"
												{{else}}href="/search?page={{last}}"
												{{/if}} aria-label="Next">
												<span aria-hidden="true">LAST</span>
											</a>
										</li>
									</ul>
								</nav>
							</div>



						</div>
					</div>

				</div>
			</div>

